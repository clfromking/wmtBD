
page{
  padding-bottom: 46rpx
}
.jw-container{
  width: 670rpx;
  margin: 0 auto
}
.jw-formItem{
  height: 60rpx;
  display: flex;
  margin: 20rpx auto
  /* background: red */
}
.jw-formItem-title{
  display: inline-block;
  font-size: 28rpx;
  color: #4F4F4F;
  height: 60rpx;
  line-height: 60rpx;
  width: 200rpx;
  min-width: 200rpx
}
.jw-formItem-input,picker{
  height: 60rpx;
  line-height: 60rpx;
  font-size: 24rpx;
  color: #4F4F4F;
}
picker{
  width: 100%;

}
.jw-chooseImgView{
  width: 558rpx;
  height: 242rpx;
  background: #E8E8E8;
}

.jw-chooseImgView-text{
  line-height: 242rpx;
  text-align: center;
  font-size: 24rpx;
  color: #696868;
  display: inline-block;
  width: 100%
}
.jw-license{
  width: 100%;
  height: 100%;
  display: block
}
textarea{
  background: #FEFEFE;
  border: 1rpx solid #D9D8D8;
  width: 558rpx;
  height: 242rpx;
  font-size: 24rpx;
  color: #4F4F4F;
  /* line-height: 24rpx; */
  padding: 6rpx;
  box-sizing: border-box
}

.jw-submitBtn{
  width: 587rpx;
  height: 86rpx;
  line-height: 86rpx;
  text-align: center;
  color: #262626;
  font-size: 36rpx;
  margin: 128rpx auto 0;
  border: 1rpx solid #AAAAAA;
  border-radius: 17rpx;
}