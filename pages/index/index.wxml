    <view class='pr'>
      <view  class=''>
        <image class='jw-options-img'></image>
        <view class='jw-options-text'>我的商户</view>
      </view>
      <button bindtap='goNav' data-index='0' form-type='submit' class='getFormIdBtn'></button>
