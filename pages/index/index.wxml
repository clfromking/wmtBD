        <view class='jw-options-text'>发现商品</view>
      </view>
      <button bindtap='goNav' data-index='1' form-type='submit' class='getFormIdBtn'></button>
    </view>
    
  </form>

  <form report-submit='true' class='jw-options-item' bindsubmit='getFormid'>
    <view class='pr'>
       <view  class=''>
        <image class='jw-options-img'></image>
        <view class='jw-options-text'>个人信息</view>
      </view>
      <button bindtap='goNav' data-index='2' form-type='submit' class='getFormIdBtn'></button>
    </view>
   
  </form>

  <form report-submit='true' class='jw-options-item' bindsubmit='getFormid'>
    <view class='pr'>
       <view  class=''>
        <image class='jw-options-img'></image>
        <view class='jw-options-text'>我的下属</view>
      </view>
      <button bindtap='goNav' data-index='3' form-type='submit' class='getFormIdBtn'></button>
