  </view>
</form>

<view class='jw-bdMsg-view'>
  <view class='jw-bdMsg-item'>
    <image class='jw-bdMsg-img'></image>
    <view class='jw-bdMsg-text'>{{bdName}}</view>
  </view>
  <view class='jw-bdMsg-item'>
    <image class='jw-bdMsg-img'></image>
    <view class='jw-bdMsg-text'>{{bdGroup}}</view>
  </view>
  <view class='jw-bdMsg-item'>
    <image class='jw-bdMsg-img'></image>
    <view class='jw-bdMsg-text'>{{bdDate}}</view>
  </view>
</view>

<view class='jw-options-view'>
  
