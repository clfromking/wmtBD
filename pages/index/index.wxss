/**index.wxss**/
.jw-register{
  width: 508rpx;
  height: 82rpx;
  display: block;
  margin: 100rpx auto 0;
  text-align: center;
  background: #5ba2fd;
  border-radius: 47rpx;
  font-size: 32rpx;
  color: #FEFCFC;
  line-height: 82rpx
}
.jw-bdMsg-view{
  display: flex;
  height: 169rpx;
  margin-top: 40rpx
}
.jw-bdMsg-item{
  width: 100%;
  height: 100%;

}
.jw-bdMsg-img,.jw-options-img{
  width: 108rpx;
  height: 108rpx;
  background: #fff;
  box-shadow: 0 3rpx 36rpx -10rpx #5BA2FD;
  display: block;
  margin:0 auto;
  border-radius: 50%
}
.jw-bdMsg-text,.jw-options-text{
  font-size: 28rpx;
  color: #4F4F4F;
  text-align: center;

}
.jw-bdMsg-text{
    margin-top: 18rpx
}

.jw-options-view{
  display: flex;
  flex-wrap: wrap;
  margin-top: 116rpx
}
.jw-options-item{
  width: 35%;
  height: 178rpx;
  margin-bottom: 108rpx;
  display: inline-block
}
.jw-options-img{
  margin-bottom: 30rpx
}
.jw-options-item:nth-child(odd){
  margin-left: 15%
}
