/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.clearfix:after{
	content : '';
	display : block;
	clear : both;
	visibility: hidden;
	height: 0;
}
.left{
	float : left;
}
.right{
	float : right;
}