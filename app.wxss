/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.clearfix:after{
	content : '';
	display : block;
	clear : both;
	visibility: hidden;
	height: 0;
}
.left{
	float : left;
}
.right{
	float : right;
}

.pr{
  position: relative !important
}
.getFormIdBtn{
  position: absolute;
  width: 100%;
  height: 100%;
  top:0;
  opacity: 0;
  
}

.red{
  color: red
}
.blue{
  color: #5BA2FD !important;
}
.xing{
  color: red;
  margin-right: 6rpx
}

.placeholderInput{
  color: #aeaeae !important
}

.ellipsis{
  overflow: hidden;
  white-space: nowrap;
  text-overflow:ellipsis;
}